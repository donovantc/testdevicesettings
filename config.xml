<?xml version='1.0' encoding='utf-8'?>
<widget id="testdevicesettings" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Test Device Settings</name>
    <description>
    </description>
    <author email="" href="">
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="black" />
    <preference name="iosExtraFilesystems" value="library,library-nosync,documents,documents-nosync,cache,bundle,root" />
    <plugin name="cordova-plugin-device" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-app-version" spec="~0.1.8" />
    <plugin name="cordova-plugin-geolocation" spec="~2.1.0" />
    <plugin name="de.appplant.cordova.plugin.badge" spec="https://github.com/katzer/cordova-plugin-badge.git" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" />
    <plugin name="com.cordova.plugins.cookiemaster" spec="https://github.com/kristianhristov/cordova-cookie-master.git" />
    <plugin name="com.adi.plugin.keychainutil" spec="~0.0.1" />
    <plugin name="cordova-plugin-camera" spec="~2.1.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.2" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.5.0" />
</widget>
